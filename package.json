{
	"name": "@swiizyy/plugin-botlist",
	"version": "1.3.4",
	"description": "Plugin for @sapphire/framework to post stats to several discord bot lists.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"module": "dist/index.mjs",
	"typedocMain": "src/index.ts",
	"author": "Swiizyy <https://github.com/swiizyy>",
	"license": "MIT",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./register": {
			"import": "./register.mjs",
			"require": "./register.js"
		}
	},
	"sideEffects": [
		"./dist/register.js",
		"./register.js",
		"./register.mjs"
	],
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"format": "prettier --write src/**/*.ts README.md package.json scripts/**/*",
		"docs": "typedoc",
		"update": "yarn upgrade-interactive",
		"build": "tsc -b src && yarn esm:register && yarn esm:default",
		"esm:register": "gen-esm-wrapper dist/register.js dist/register.mjs",
		"esm:default": "gen-esm-wrapper dist/index.js dist/index.mjs",
		"clean": "tsc -b src --clean",
		"watch": "tsc -b src -w",
		"bump": "cliff-jumper",
		"check-update": "cliff-jumper --dry-run",
		"check-botlists": "node scripts/check.mjs",
		"sversion": "standard-version",
		"commit": "git-cz",
		"cz": "git-cz",
		"postinstall": "husky install .github/husky",
		"prepack": "yarn build && pinst --disable",
		"postpack": "pinst --enable"
	},
	"dependencies": {
		"tiny-typed-emitter": "^2.1.0",
		"tslib": "^2.6.2",
		"undici": "^6.18.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@favware/cliff-jumper": "^3.0.3",
		"@favware/npm-deprecate": "^1.0.7",
		"@sapphire/eslint-config": "^5.0.4",
		"@sapphire/framework": "^5.2.1",
		"@sapphire/prettier-config": "^2.0.0",
		"@sapphire/ts-config": "^5.0.1",
		"@types/node": "^20.14.0",
		"@typescript-eslint/eslint-plugin": "^7.11.0",
		"@typescript-eslint/parser": "^7.11.0",
		"colorette": "^2.0.20",
		"cz-conventional-changelog": "^3.3.0",
		"discord.js": "^14.15.3",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"gen-esm-wrapper": "^1.1.3",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.5",
		"pinst": "^3.0.0",
		"prettier": "^3.3.0",
		"pretty-quick": "^4.0.0",
		"standard-version": "^9.5.0",
		"typedoc": "^0.25.13",
		"typedoc-plugin-mdn-links": "^3.1.28",
		"typescript": "^5.4.5"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/swiizyy/sapphire-plugin-botlist.git"
	},
	"files": [
		"dist",
		"!dist/*.tsbuildinfo",
		"register.*"
	],
	"engines": {
		"node": ">=v16.6.0",
		"npm": ">=7.0.0"
	},
	"keywords": [
		"sapphiredev",
		"bot",
		"typescript",
		"ts",
		"yarn",
		"discord",
		"sapphire",
		"discordjs",
		"botlist",
		"topgg"
	],
	"bugs": {
		"url": "https://github.com/swiizyy/sapphire-plugin-botlist/issues"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@3.3.1"
}
